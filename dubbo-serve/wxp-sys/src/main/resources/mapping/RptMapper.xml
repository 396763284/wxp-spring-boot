<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="per.wxp.wxpsys.mapper.RptMapper" >

  <select id="getRptHeadList" parameterType="Map" resultType="Map" >
    SELECT *
    FROM
    wa_rpt_head_code
    WHERE rpt_code =#{rpt_code}
    AND state='1'
    ORDER BY sort
  </select>


</mapper>